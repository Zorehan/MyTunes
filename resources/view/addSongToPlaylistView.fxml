<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="456.0" prefWidth="227.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.Controller.songToPlaylistController">
   <children>
      <Label alignment="CENTER" layoutX="14.0" prefHeight="40.0" prefWidth="200.0" text="Select a playlist">
         <font>
            <Font name="Microsoft JhengHei" size="20.0" />
         </font>
      </Label>
      <Button layoutX="14.0" layoutY="409.0" mnemonicParsing="false" onAction="#clickAddSong" prefHeight="34.0" prefWidth="68.0" text="Add song" />
      <Button layoutX="146.0" layoutY="409.0" mnemonicParsing="false" onAction="#clickCancel" prefHeight="34.0" prefWidth="68.0" text="Cancel" />
      <TableView fx:id="tblPlaylists" layoutX="14.0" layoutY="35.0" prefHeight="366.0" prefWidth="200.0">
        <columns>
          <TableColumn fx:id="colName" prefWidth="199.0" text="Name" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
