<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="769.0" prefWidth="1125.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.Controller.mainMyTunesViewController">
   <children>
      <TableView fx:id="tblPlaylists" layoutX="14.0" layoutY="69.0" prefHeight="486.0" prefWidth="220.0">
        <columns>
          <TableColumn fx:id="colName" onEditCommit="#editPlaylistName" prefWidth="108.0" text="Name" />
          <TableColumn fx:id="colSongs" prefWidth="41.0" text="Songs" />
            <TableColumn fx:id="colTimePlaylist" prefWidth="76.0" text="Time" />
        </columns>
      </TableView>
      <Label layoutX="14.0" layoutY="43.0" text="Playlists" />
      <Label layoutX="370.0" layoutY="43.0" text="Songs on Playlist" />
      <Button fx:id="btnSongToPlaylist" layoutX="730.0" layoutY="73.0" mnemonicParsing="false" onAction="#clickSongToPlaylist" prefHeight="25.0" prefWidth="30.0" text="&lt;-" />
      <Label layoutX="775.0" layoutY="43.0" text="Songs" />
      <Button fx:id="btnNewPlayList" layoutX="15.0" layoutY="564.0" mnemonicParsing="false" onAction="#clickNewPlaylist" text="New" />
      <Button fx:id="btnDeletePlaylist" layoutX="188.0" layoutY="564.0" mnemonicParsing="false" onAction="#clickDeletePlaylist" text="Delete" />
      <Button fx:id="btnMoveDownPlaylist" layoutX="370.0" layoutY="564.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="14.0" text="Button" />
      <Button fx:id="btnMoveUpPlaylist" layoutX="415.0" layoutY="564.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="24.0" text="Button" />
      <Button fx:id="btnDeleteFromPlaylist" layoutX="608.0" layoutY="564.0" mnemonicParsing="false" text="Delete" />
      <Button fx:id="btnNewSong" layoutX="771.0" layoutY="564.0" mnemonicParsing="false" onAction="#clickNewSong" text="New" />
      <Button fx:id="btnDeleteSongs" layoutX="916.0" layoutY="564.0" mnemonicParsing="false" onAction="#clickDeleteSong" text="Delete" />
      <Button fx:id="btnClose" layoutX="1063.0" layoutY="564.0" mnemonicParsing="false" text="Close" />
      <TableView fx:id="tblSongs" layoutX="771.0" layoutY="73.0" prefHeight="486.0" prefWidth="340.0">
        <columns>
          <TableColumn fx:id="colTitle" onEditCommit="#editSongName" prefWidth="197.0" text="Title" />
          <TableColumn fx:id="colArtist" onEditCommit="#editArtistName" prefWidth="129.0" text="Artist" />
            <TableColumn fx:id="colCategory" onEditCommit="#editCategoryName" prefWidth="87.0" text="Category" />
            <TableColumn fx:id="colTimeSongs" prefWidth="54.0" text="Time" />
        </columns>
      </TableView>
      <Label layoutX="916.0" layoutY="14.0" text="Filter:" />
      <TextField fx:id="txtSongSearch" layoutX="958.0" layoutY="10.0" prefHeight="25.0" prefWidth="115.0" />
      <Button fx:id="btnSearch" layoutX="1085.0" layoutY="10.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="0.0" text="Button" />
      <Button fx:id="btnPlaySong" layoutX="476.0" layoutY="668.0" mnemonicParsing="false" onAction="#clickPlaySong" prefHeight="42.0" prefWidth="106.0" text="PLAY" />
      <Button fx:id="btnPauseSong" layoutX="351.0" layoutY="668.0" mnemonicParsing="false" onAction="#clickPauseSong" prefHeight="42.0" prefWidth="106.0" text="PAUSE" />
      <Button fx:id="btnStopSong" layoutX="600.0" layoutY="668.0" mnemonicParsing="false" onAction="#clickStopSong" prefHeight="42.0" prefWidth="106.0" text="STOP" />
      <ProgressBar fx:id="songBar" layoutX="220.0" layoutY="719.0" prefHeight="25.0" prefWidth="618.0" progress="0.0" />
      <TableView fx:id="tblSongsOnPlaylist" layoutX="370.0" layoutY="69.0" prefHeight="486.0" prefWidth="289.0">
        <columns>
          <TableColumn fx:id="colSongsOnPlaylistTitle" prefWidth="75.0" text="Title" />
          <TableColumn fx:id="colTimeSongsOnPlaylist" prefWidth="75.0" text="Time" />
        </columns>
      </TableView>
      <Slider fx:id="volumeSlider" layoutX="946.0" layoutY="725.0" onDragDetected="#changeVolume" value="50.0" />
   </children>
</AnchorPane>
